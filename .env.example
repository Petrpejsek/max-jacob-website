# Environment variables for Max & Jacob
# Copy this to .env and add your actual API keys

# OpenRouter API key for LLM assistants (required for audit pipeline)
# Get your key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Resend API key for email sending (required for email outreach)
# Get your key from: https://resend.com/api-keys
RESEND_API_KEY=your_resend_api_key_here

# Admin password (required for accessing /admin routes)
ADMIN_PASSWORD=your_admin_password_here

# Session secret (optional - fallback used if not set)
SESSION_SECRET=your_session_secret_here

# Node environment
NODE_ENV=development
